script/source = "extends KinematicBody2D
###################### EXPORTS ######################
export (int) var walk_speed
export (int) var on_air_speed
export (int) var jump_force
export (float) var friction #Friction values go between 0 and 1
export (float) var on_air_friction

###################### CONSTANTS ######################
const GRAVITY = 9.8

###################### INTERNAL VARIABLES ######################
var velocity = Vector2(0,0)
var motion = 0
var floorContacts = 0

func _ready():
	# We need to set the camera first
	$Camera2D.make_current()

func _physics_process(delta):
	#Applying gravity
	velocity.y += GRAVITY
	
	###################### WALK ######################
	if Input.is_action_pressed(\"ui_left\"):
		if floorContacts > 0:
			velocity.x -= walk_speed
		else:
			velocity.x -= on_air_speed
	elif Input.is_action_pressed(\"ui_right\"):
		if floorContacts > 0:
			velocity.x += walk_speed
		else:
			velocity.x += on_air_speed
	else:
		###################### FRICTION ######################
		if floorContacts > 0:
			velocity.x *= friction
		else:
			velocity.x *= on_air_friction
	
	###################### JUMP ######################
	if floorContacts > 0:
		if Input.is_action_pressed(\"ui_up\"):
			velocity.y = -jump_force
		elif Input.is_action_pressed(\"ui_down\"):
			pass
	move_and_slide(velocity, Vector2(0,-1))
	
	###################### UPDATE ORIENTATION ######################
	if velocity.x > 0.1:
		$AnimatedSprite.flip_h = false
	elif velocity.x < 0.1:
		$AnimatedSprite.flip_h = true
		

func _on_FloorDetection_body_entered(body):
	floorContacts += 1

func _on_FloorDetection_body_exited(body):
	floorContacts -= 1
"

