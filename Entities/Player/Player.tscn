script/source = "extends KinematicBody2D

# This demo shows how to build a kinematic controller.

###### CONSTANTS
const GRAVITY = 500.0 # pixels/second/second
# Angle in degrees towards either side that the player can consider \"floor\"
const FLOOR_ANGLE_TOLERANCE = 40
const WALK_FORCE = 600
const WALK_MIN_SPEED = 10
const WALK_MAX_SPEED = 200
const RUN_MIN_SPEED = 40
const RUN_MAX_SPEED = 500
const STOP_FORCE = 1300
const JUMP_SPEED = 350
const JUMP_MAX_AIRBORNE_TIME = 0.2
const SLIDE_STOP_VELOCITY = 1.0 # one pixel/second
const SLIDE_STOP_MIN_TRAVEL = 1.0 # one pixel

var velocity = Vector2()
var on_air_time = 100
var jumping = false

var prev_jump_pressed = false
var bulletScene = preload(\"res://Entities/Skills and projectiles/Bullet.tscn\")
var bullet = null
var maxBullets = 6
var numBullets = 6
var weaponReady = true
var reloading = false

var HP = 100
var MP = 100
##############################################################################################
func _ready():
	$Camera2D.make_current()
##############################################################################################
func _physics_process(delta):
	var shoot = Input.is_action_pressed(\"shoot\")
	player_move(delta)
	###### Orientation
	if velocity.x > 0.1:
		$AnimatedSprite.flip_h = false
		$GunShootFlash.position.x = 20
	elif velocity.x < -0.1:
		$AnimatedSprite.flip_h = true
		$GunShootFlash.position.x = -20
	###### Skills
	if shoot:
		shoot()
	#Update GUI
	$GUI.setHP(HP)
	$GUI.setMP(100*numBullets/maxBullets)
##############################################################################################
func _on_ReloadMagazine_timeout():
	numBullets = maxBullets
	weaponReady = true
func _on_FireRate_timeout():
	weaponReady = true
##############################################################################################
func player_move(delta):
	var walk_left = Input.is_action_pressed(\"move_left\")
	var walk_right = Input.is_action_pressed(\"move_right\")
	var jump = Input.is_action_pressed(\"jump\")
	var run = Input.is_action_pressed(\"run\")
	var stop = true
	# Create forces
	var force = Vector2(0, GRAVITY)
	if walk_left:
		if run:
			if velocity.x <= RUN_MIN_SPEED and velocity.x > -RUN_MAX_SPEED:
				force.x -= WALK_FORCE
				stop = false
		elif velocity.x <= WALK_MIN_SPEED and velocity.x > -WALK_MAX_SPEED:
			force.x -= WALK_FORCE
			stop = false
	elif walk_right:
		if run:
			if velocity.x >= -RUN_MIN_SPEED and velocity.x < RUN_MAX_SPEED:
				force.x += WALK_FORCE
				stop = false
		if velocity.x >= -WALK_MIN_SPEED and velocity.x < WALK_MAX_SPEED:
			force.x += WALK_FORCE
			stop = false
	
	if stop:
		var vsign = sign(velocity.x)
		var vlen = abs(velocity.x)
		
		vlen -= STOP_FORCE * delta
		if vlen < 0:
			vlen = 0
		
		velocity.x = vlen * vsign
	
	# Integrate forces to velocity
	velocity += force * delta	
	# Integrate velocity into motion and move
	velocity = move_and_slide(velocity, Vector2(0, -1))
	
	if is_on_floor():
		on_air_time = 0
		
	if jumping and velocity.y > 0:
		# If falling, no longer jumping
		jumping = false
	
	if on_air_time < JUMP_MAX_AIRBORNE_TIME and jump and not prev_jump_pressed and not jumping:
		# Jump must also be allowed to happen if the character left the floor a little bit ago.
		# Makes controls more snappy.
		velocity.y = -JUMP_SPEED
		jumping = true
	
	on_air_time += delta
	prev_jump_pressed = jump
##############################################################################################
func shoot():
	if reloading:
		reloading = false
		$Reload.play()
		$Timers/ReloadMagazine.start()
	elif weaponReady:
		bullet = bulletScene.instance()
		
		$GunShootFlash/GunShootFlashAnimation.play(\"GunFlashAnimation\")
		bullet.position = global_position
		if(!$AnimatedSprite.flip_h):
			bullet.velocity.x *= -1
		get_parent().add_child(bullet)
		bullet = null
		numBullets-=1
		weaponReady = false
		if numBullets > 0:
			$Timers/FireRate.start()
		else:
			reloading = true"

