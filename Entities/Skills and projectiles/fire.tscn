code = "shader_type canvas_item;

// 2D Random
float rand(float i){
	return fract(sin(i)*100000.0);
}

float noise(float x){
	float i = floor(x);  // integer
	float f = fract(x);  // fraction
	float y;
	y = rand(i); //rand() is described in the previous chapter
	y = mix(rand(i), rand(i + 1.0), f);
	y = mix(rand(i), rand(i + 1.0), smoothstep(0.0,1.0,f));
	return y;
}


void vertex(){
	COLOR = vec4(vec3(rand () * 10.0), .0);
}"

